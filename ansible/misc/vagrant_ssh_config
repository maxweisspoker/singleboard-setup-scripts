Host archvagrant1
  HostName 127.0.0.1
  User vagrant
  Port 2222
  UserKnownHostsFile /dev/null
  StrictHostKeyChecking no
  PasswordAuthentication no
  IdentityFile ./.vagrant/machines/archvagrant1/virtualbox/private_key
  IdentitiesOnly yes
  LogLevel FATAL

